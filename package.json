{
  "name": "art.simian",
  "version": "1.0.0",
  "private": true,
  "description": "art.simian wechat mini app",
  "scripts": {
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/components": "1.2.14",
    "@tarojs/redux": "^1.3.17",
    "@tarojs/redux-h5": "^1.3.17",
    "@tarojs/router": "1.2.14",
    "@tarojs/taro": "1.2.14",
    "@tarojs/taro-alipay": "1.2.14",
    "@tarojs/taro-h5": "1.2.14",
    "@tarojs/taro-swan": "1.2.14",
    "@tarojs/taro-tt": "1.2.14",
    "@tarojs/taro-weapp": "1.2.14",
    "immer": "^8.0.1",
    "nerv-devtools": "^1.3.9",
    "nervjs": "^1.3.9",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "1.2.14",
    "@tarojs/plugin-csso": "1.2.14",
    "@tarojs/plugin-less": "1.2.14",
    "@tarojs/plugin-uglifyjs": "1.2.14",
    "@tarojs/webpack-runner": "1.2.14",
    "@types/react": "^16.4.6",
    "@types/redux-logger": "^3.0.7",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-taro": "^1.3.19",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-taro": "1.2.14",
    "eslint-plugin-typescript": "^0.12.0",
    "prettier": "^1.18.2",
    "typescript": "^3.0.1"
  }
